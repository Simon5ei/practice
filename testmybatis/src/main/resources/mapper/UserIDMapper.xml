<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.practice.testmybatis.mapper.UserIDMapper">
    <select id="queryUserIDMapper" resultType="user" parameterType="java.lang.Integer">
        select *
        from user
        where iduser = #{ID}
    </select>
</mapper>
